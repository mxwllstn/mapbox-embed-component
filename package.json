{
  "name": "@mxwllstn/mapbox-embed-vue",
  "version": "0.0.35",
  "author": "Max Stein <maxwell.stein@gmail.com> (https://maxstein.net",
  "description": "Vue component that embeds a mapbox map with a marker",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/mapbox-embed.umd.js",
  "types": "./dist/src/components/index.d.ts",
  "module": "./dist/mapbox-embed.es.js",
  "exports": {
    ".": {
      "import": "./dist/mapbox-embed.es.js",
      "require": "./dist/mapbox-embed.umd.js"
    }
