{
  "name": "@mxwllstn/mapbox-embed-vue",
  "type": "module",
  "version": "0.0.89",
  "description": "Vue component that embeds a mapbox map with a marker",
  "author": "Max Stein <maxwell.stein@gmail.com> (https://maxstein.net",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxwllstn/mapbox-embed-vue"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/mapbox-embed.mjs"
      },
      "require": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/mapbox-embed.cjs"
      }
    }
  },
  "main": "./dist/mapbox-embed.cjs",
  "module": "./dist/mapbox-embed.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublish": "yarn lint && yarn build"
  },
  "dependencies": {
    "@turf/turf": "^7.0.0-alpha.114",
    "@types/mapbox-gl": "^3.1.0",
    "mapbox-gl": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.2",
    "@babel/types": "^7.24.5",
    "@types/node": "^20.12.10",
    "@types/svgo": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.2.0",
    "postcss-html": "^1.6.0",
    "sass": "^1.77.0",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.4.27",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.16"
  }
}
